<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
       http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    <dubbo:application name="test-provider"/>
    <dubbo:registry address="zookeeper://172.16.61.128:2181" check="false" version="1.0"
                    timeout="3000"></dubbo:registry>
    <!-- 要暴露的服务接口 -->
    <dubbo:service interface="org.yabo.service.test.TestService" ref="testService"/>

</beans>